{
  "name": "ssfp",
  "version": "0.0.1",
  "description": "Stop Shitty Facebook Photos",
  "manifest_version": 2,
  "content_scripts": [{
    "all_frames": true,
    "run_at": "document_start",
    "matches": ["*://*.facebook.com/*"],
    "js": ["vendor/jquery.min.js", "ssfp.js"]
  }],
  "permissions": [
    "tabs"
  ]
}